<script type="text/javascript">

  var mots = ["séminaires", "salons", "événements", "..."];

  // Fonction pour changer le mot automatiquement
  function changerMot() {
    var motActuel = document.getElementById("motChangeant").textContent;
    var indexMotSuivant = mots.indexOf(motActuel) + 1;
    if (indexMotSuivant === mots.length) {
      indexMotSuivant = 0;
    }
    document.getElementById("motChangeant").textContent = mots[indexMotSuivant];
  }

  // Appel de la fonction toutes les deux secondes
  setInterval(changerMot, 2000);

</script>
